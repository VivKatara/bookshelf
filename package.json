{
  "name": "book_shelf",
  "version": "1.0.0",
  "description": "Virtual bookshelf to share what you're currently reading, have read, or want to read in the future",
  "main": "server.js",
  "scripts": {
    "client": "cd client && npm run start",
    "dev": "concurrently \"nodemon server.js\" \"nodemon authServer.js\" \"npm run client\"",
    "devServer": "nodemon server.js",
    "devAuthServer": "nodemon authServer.js"
  },
  "author": "Vivek Katara",
  "license": "ISC",
  "dependencies": {
    "axios": "0.19.2",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "is-empty": "1.2.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.9.17",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "validator": "13.0.0"
  },
  "devDependencies": {
    "concurrently": "5.2.0",
    "nodemon": "2.0.4"
  }
}
